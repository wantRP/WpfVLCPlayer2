<Window x:Name="close" x:Class="WpfApp1.Playlist"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="playlist" Height="287" Width="204" WindowStyle="None" MouseMove="close_MouseMove">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="28"/>
			<RowDefinition Height="*"/>
			<RowDefinition Height="20"/>
		</Grid.RowDefinitions>
		<TextBlock Grid.Row="0" Text="播放列表" Background="White" FontSize="14" VerticalAlignment="Top"/>
		<Button Grid.Row="2" x:Name="buttontest" Content="test" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="buttontest_Click"/>
		
	</Grid>
</Window>
